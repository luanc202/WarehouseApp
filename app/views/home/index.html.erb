Galpões & Estoque
<section>
  <h2>Galpões</h2>
    <% @warehouses.each do |w| %>
  <div>
      <h3><%= link_to(w.name, warehouse_path(w.id)) %></h3>
      <dl>
        <dt>Código</dt>
        <dd><%= w.code %></dd>
        <dt>Cidade</dt>
        <dd><%= w.city %></dd>
        <dt>Área</dt>
        <dd><%= w.area %></dd>
      </dl>
    <% end %>
  </div>

  <% if @warehouses.empty? %>
    <h2>Não existem galpões cadastrados</h2>
  <% end %>
</section>